<launch>
    <arg name="map_file" default="$(find coverage_planning)/maps/simple.yaml"/>

    <node pkg="map_server" name="map_server" type="map_server" args="$(arg map_file)" respawn="true" output="screen"/>
    <node pkg="coverage_planning" type="optimizer.py" name="optimizer" clear_params="true" output="screen">
        <param name="from_map_file" value="false"/>
        <param name="map_file" value="$(arg map_file)"/>
        <param name="cardinality_gain" value="80"/>
        <param name="time_gain" value="1"/>
    </node>
</launch>
